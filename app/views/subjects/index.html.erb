<p id="notice"><%= notice %></p>

<h1>Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Final mark</th>
      <th>Exame date and time</th>
      <th>Exam venue</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= subject.name %></td>
        <td><%= subject.code %></td>
        <td><%= subject.final_Mark %></td>
         <td><%= subject.exam_datetime %></td>
        <td><%= subject.exam_venue %></td>
        <td><%= subject.student_id %></td>
        <td><%= link_to 'Show', student_subject_path(current_student,subject) %></td>
        <td><%= link_to 'Edit', edit_student_subject_path(current_student,subject ) %></td>
        <td><%= link_to 'Destroy', student_subject_path(current_student,subject ), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


